---
import dk from "../dk.svg";
import Layout from "@layouts/Layout.astro";
import Container from "@layouts/Container.astro";
import { getEntry } from "astro:content";
const zulassung = await getEntry("pages", "zulassung");
const { Content, headings } = await zulassung.render();
const { title, description, color } = zulassung.data;
---

<Layout {title} {description} {color}>
  <div
    class="flex flex-col items-center justify-center text-yellow prose-headings:text-green marker:text-yellow prose-a:text-yellow"
  >
    <Container>
      <Content />
    </Container>
  </div>
</Layout>
