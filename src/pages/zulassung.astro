---
import dk from "../dk.svg";
import Layout from "@layouts/Layout.astro";
import Container from "@layouts/Container.astro";
import { getEntry } from "astro:content";
const zulassung = await getEntry("pages", "zulassung");
const { Content, headings } = await zulassung.render();
---

<Layout title="Zulassung" description="Test">
  <div
    class="flex flex-col items-center justify-center text-green prose-headings:text-green prose-a:text-green prose-p:text-lg"
  >
    <Container>
      <Content />
    </Container>
  </div>
</Layout>
